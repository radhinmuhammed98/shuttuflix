{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-src 'self' https://www.vidking.net https://vidking.net; script-src 'self' 'unsafe-inline' https://www.vidking.net https://vidking.net; connect-src 'self' https://api.themoviedb.org"
        }
      ]
    }
  ],
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/data/(.*)", "headers": { "content-type": "application/json" } },
    { "src": "/css/(.*)", "headers": { "content-type": "text/css" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "public"
}

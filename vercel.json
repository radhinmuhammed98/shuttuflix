{
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/api/sources", "dest": "/api/sources.js" },
    { "src": "/data/(.*)", "headers": { "content-type": "application/json" } },
    { "src": "/css/(.*)", "headers": { "content-type": "text/css" } },
    { "src": "/js/(.*)", "headers": { "content-type": "application/javascript" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "public"
}

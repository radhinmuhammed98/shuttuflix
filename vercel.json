{
  "routes": [
    { "src": "/api/search", "dest": "/api/search.js" },
    { "src": "/api/proxy", "dest": "/api/proxy.js" },
    { "src": "/data/(.*)", "headers": { "content-type": "application/json" } },
    { "src": "/css/style.css", "headers": { "content-type": "text/css" } },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "public"
}
